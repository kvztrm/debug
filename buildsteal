-- Executor script: TP to CENTER of HIGHEST PRIORITY brainrot/plot (Strawberry Elephant = #1)

local Workspace = game:GetService("Workspace")
local Players = game:GetService("Players")
local LocalPlayer = Players.LocalPlayer

local character = LocalPlayer.Character or LocalPlayer.CharacterAdded:Wait()
local hrp = character:WaitForChild("HumanoidRootPart")  -- HumanoidRootPart

local PlotsFolder = Workspace:FindFirstChild("Plots")
if not PlotsFolder then
    print("‚ùå No 'Plots' folder found in Workspace!")
    return
end

-- PRIORITY (1 = highest ‚Üí teleports here first if found)
local targets = {
    {name = "Strawberry Elephant", prio = 1},
    {name = "Torrtuginni Dragonfruitinni", prio = 2},
    {name = "Chillin Chilli", prio = 3},
    {name = "Alessio", prio = 4},
    {name = "Bulbito Bandito Traktorito", prio = 5},
    {name = "Rhino Toasterino", prio = 6},
    {name = "Octamasu Noddledudu", prio = 7},
    {name = "KarKirKirKurKur", prio = 8},
    {name = "La grande Combinasion", prio = 9}
}

-- Approximate CENTER positions of plots 1-6 (middle of the plot base)
-- These are common-ish coords from Steal a Brainrot videos/guides ‚Äî adjust if off!
local plotCenters = {
    ["1"] = Vector3.new(-170, 7, 194),   -- Plot 1 center
    ["2"] = Vector3.new(-282, 7, 126),   -- Plot 2 center
    ["3"] = Vector3.new(-281, 7, -54),   -- Plot 3 center
    ["4"] = Vector3.new(-182, 7, -133),  -- Plot 4 center
    ["5"] = Vector3.new(-70, 7, -66),    -- Plot 5 center
    ["6"] = Vector3.new(-71, 7, 114)     -- Plot 6 center
}

print("üîç Scanning plots 1-6 for brainrots (highest prio first)...")

local best = nil
local bestPrio = math.huge

for num = 1, 6 do
    local plotName = tostring(num)
    local plot = PlotsFolder:FindFirstChild(plotName)
    if not plot then continue end
    
    -- Path to sign text (Sign ‚Üí SignBlock ‚Üí BaseSign ‚Üí Text1)
    local txtObj = plot.Sign
    if txtObj then txtObj = txtObj.SignBlock end
    if txtObj then txtObj = txtObj.BaseSign end
    if txtObj then txtObj = txtObj.Text1 end
    
    local text = ""
    if txtObj and txtObj:IsA("TextLabel") then
        text = txtObj.Text or ""
    end
    
    -- Check each target
    for _, t in ipairs(targets) do
        if text:lower():find(t.name:lower()) then
            if t.prio < bestPrio then
                bestPrio = t.prio
                best = {
                    plot = num,
                    name = t.name,
                    pos   = plotCenters[plotName]
                }
                print("Found #" .. t.prio .. ": " .. t.name .. " ‚Üí Plot " .. num)
            end
            break  -- one match per plot
        end
    end
end

if best then
    print("\nüöÄ TP to CENTER of " .. best.name .. " (Plot " .. best.plot .. ")")
    print("   Exact pos: " .. tostring(best.pos))
    
    -- Teleport DEAD CENTER (no extra height)
    hrp.CFrame = CFrame.new(best.pos)
    
    print("‚úÖ Teleported to brainrot center!")
else
    print("\n‚ùå No matching brainrots found in plots 1-6")
end
